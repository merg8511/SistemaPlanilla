@model string

<div class="mb-3">
    <button type="submit" class="btn btn-primary btn-icon-split" onfocus="false" asp-route-id="@Model">
        <span class="icon text-white-50">
            <i class="bi bi-floppy"></i>
        </span>
        <span class="text">Save</span>
    </button>
    <a asp-action="Index" class="btn btn-secondary btn-icon-split">
        <span class="icon text-white-50">
            <i class="bi bi-arrow-left-circle"></i>
        </span>
        <span class="text">Return to Index</span>
    </a>
</div>