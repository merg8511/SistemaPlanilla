@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SistemaOrbita</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/sb-admin-2.css" asp-append-version="true" />

    <link rel="icon" href="~/img/orbita/logo-orbita-icono.svg" type="image/x-icon" sizes="32x32">
    <!--BOOTSTRAP ICONS-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">

    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!--BOOTSTRAP ICONS-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
      
</head>
<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="#">
                <div class="sidebar-brand-icon">
                    <img src="~/img/orbita/logo-orbita-icono.svg" width="50" />
                </div>
                <div class="sidebar-brand-text" style="font-size:20px">rbita</div>
            </a>

            
        </ul>


        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>                 

                </nav>
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid pb-3">
                    <div class="text-center">
                        <p class="lead">
                            <div>
                                <img src="~/img/pages/undraw_search_app_oso2.svg" class="text-center w-25" />
                            </div>
                            <div class="mt-3">
                                <h1 class="text-danger">@ViewData["Title"]</h1>
                                <p>This account has been locked out, please try again later.</p>
                            </div>
                            <a class="btn btn-primary" asp-area="BusinessManagement" asp-controller="Homr" asp-action="Index">Login</a>
                        </p>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    @await Component.InvokeAsync("Logout")

    <!-- Core plugin JavaScript-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Bootstrap core JavaScript-->
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>*@
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
    <!-- Notifications-->
    
    <script src="~/js/sb-admin-2.js" asp-append-version="true"></script>

    <!-- Page level plugins -->
   
    <script src="~/lib/select2/js/select2.min.js"></script>
</body>
</html>
